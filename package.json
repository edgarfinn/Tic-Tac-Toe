{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic Tac Toe (a.k.a Noughts and Crosses). Everyones favourite childhood game",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "webpack-dev-server --hot --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --display-error-details --config webpack.prod.js",
    "postinstall": "NODE_ENV=production webpack --display-error-details --config webpack.prod.js",
    "lint:css": "stylelint './src/**/*.js'",
    "test": "jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgarfinn/Tic-Tac-Toe.git"
  },
  "keywords": [
    "Game",
    "React",
    "Jest",
    "Webpack"
  ],
  "author": "edgarfinn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edgarfinn/Tic-Tac-Toe/issues"
  },
  "homepage": "https://github.com/edgarfinn/Tic-Tac-Toe#readme",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.0.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "styled-components": "^3.2.5",
